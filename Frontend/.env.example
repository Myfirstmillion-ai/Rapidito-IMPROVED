# URL del servidor backend
VITE_SERVER_URL=http://localhost:4000

# Ambiente
VITE_ENVIRONMENT=development

# Timeout para viajes (en ms) - 90 segundos por defecto
VITE_RIDE_TIMEOUT=90000

# Mapbox Token (obtener en https://account.mapbox.com/)
VITE_MAPBOX_TOKEN=pk.xxx

# Google OAuth Configuration
VITE_ENABLE_GOOGLE_OAUTH=true

# Professional Tracking Configuration
# Update interval for driver location streaming (ms)
VITE_TRACKING_UPDATE_INTERVAL=4000
# Animation duration for smooth map transitions (ms)
VITE_MAP_ANIMATION_DURATION=3000
